<body class="form" style="background-color: #1b55e2;">


    <div class="form-container outer">
        <div class="form-form">
            <div class="form-form-wrap">
                <div class="form-container">
                    <div class="form-content">

                        <h1 class="">اشتراك كمستخدم جديد</h1>
                        <p class="signup-link register">هل لديك حساب? <a asp-page="./Login">تسجيل الدخول</a></p>
                        <form class="text-left" asp-route-returnUrl="@Model.ReturnUrl" method="post">
                            <div class="form">


                                <div id="email-field" class="field-wrapper input">
                                    <label for="email">الايميل</label>
                                    <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-at-sign register"><circle cx="12" cy="12" r="4"></circle><path d="M16 8v5a3 3 0 0 0 6 0v-1a10 10 0 1 0-3.92 7.94"></path></svg>
                                    @*<input id="email" name="email" type="text" value="" class="form-control" placeholder="Email">*@
                                    <input asp-for="Input.Email" class="form-control" id="email" placeholder="Email" />
                                </div>

                                <div id="email-field" class="field-wrapper input">
                                    <label for="email">كلمة السر</label>
                                    <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-lock"><rect x="3" y="11" width="18" height="11" rx="2" ry="2"></rect><path d="M7 11V7a5 5 0 0 1 10 0v4"></path></svg>
                                    @*<input id="email" name="email" type="text" value="" class="form-control" placeholder="Email">*@
                                    <input asp-for="Input.Password" class="form-control"  placeholder="كلمة السر" />
                                </div>
                                <div id="email-field" class="field-wrapper input">
                                    <label for="email">تاكيد كلمة السر</label>
                                    <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-lock"><rect x="3" y="11" width="18" height="11" rx="2" ry="2"></rect><path d="M7 11V7a5 5 0 0 1 10 0v4"></path></svg>
                                    @*<input id="email" name="email" type="text" value="" class="form-control" placeholder="Email">*@
                                    <input asp-for="Input.ConfirmPassword" class="form-control"  placeholder="تاكيد كلمة السر" />
                                </div>


                               

                                <div class="d-sm-flex justify-content-between">
                                    <div class="field-wrapper">
                                        <button type="submit" class="btn btn-primary" value="">اشتراك</button>
                                    </div>
                                </div>


                            </div>
                        </form>

                    </div>
                </div>
            </div>
        </div>
    </div>


    <!-- BEGIN GLOBAL MANDATORY SCRIPTS -->
    <script src="~/assets/js/libs/jquery-3.1.1.min.js"></script>
    <script src="~/bootstrap/js/popper.min.js"></script>
    <script src="~/bootstrap/js/bootstrap.min.js"></script>

    <!-- END GLOBAL MANDATORY SCRIPTS -->
    <script src="~/assets/js/authentication/form-2.js"></script>

</body>

